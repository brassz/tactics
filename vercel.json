{
  // Usa o app Expo dentro da pasta "mobile" como fonte do deploy
  "installCommand": "cd mobile && npm install",

  // Gera o build web estático do Expo
  "buildCommand": "cd mobile && npx expo export:web",

  // Diretório que a Vercel deve publicar (saída do expo export:web)
  "outputDirectory": "mobile/dist"
}


